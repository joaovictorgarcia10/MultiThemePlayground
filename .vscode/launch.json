    "version": "0.2.0",
    "configurations": [
        {
            "name": "MultiTheme Standard - Dev",
            "request": "launch",
            "type": "dart",
            "console": "debugConsole",
            "args": [
                "--dart-define=THEME=Standard",
                "--dart-define=ENV=dev",
            ],
        },
        {
            "name": "MultiTheme Other - Dev",
            "request": "launch",
            "type": "dart",
            "console": "debugConsole",
            "args": [
                "--dart-define=theme=Other",
                "--dart-define=env=dev",
            ],
        },
    ]
}